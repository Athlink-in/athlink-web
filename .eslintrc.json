{
    "extends": "airbnb",
    "rules" : {
        "react/jsx-filename-extension" : "off",
        "jsx-quotes" : "off",
        "jsx-a11y/anchor-is-valid": "off",
        "react/prop-types": "off",
        "react/jsx-no-constructed-context-values": "off",
        "object-curly-newline": "off",
        "linebreak-style":"off",
        "max-len":"off",
        "react/jsx-no-bind":"off"
    }
}
